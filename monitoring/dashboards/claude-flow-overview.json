{
  "dashboard": {
    "id": null,
    "title": "Claude Flow Overview",
    "tags": ["claude-flow", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "title": "Active Swarm Agents",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(claude_flow_swarm_agents{status=\"active\"})",
            "legendFormat": "Active Agents"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Task Queue Size",
        "type": "graph",
        "targets": [
          {
            "expr": "claude_flow_task_queue_size",
            "legendFormat": "{{priority}} priority"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "HTTP Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{route}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(errors_total[5m])",
            "legendFormat": "{{type}} - {{severity}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "claude_flow_memory_usage_bytes",
            "legendFormat": "{{namespace}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Neural Model Accuracy",
        "type": "gauge",
        "targets": [
          {
            "expr": "claude_flow_neural_model_accuracy",
            "legendFormat": "{{model_type}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      }
    ]
  }
}